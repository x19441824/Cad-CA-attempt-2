<p style="color: green"><%= notice %></p>

<h1>Clients</h1>

<div id="clients">
  <% @clients.each do |client| %>
  <tr>
    <td><%= client.name %></td>
    <td><%= client.email %></td>
    <td><%= client.phone_number %></td>
    <!-- Other fields -->
    <%= link_to "Show this client", client %>
  </tr>
  <% end %>
</div>

<%= link_to "New client", new_client_path %>
